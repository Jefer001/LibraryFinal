@model Library.DAL.Entities.University

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estás seguro que deseas eliminar esta universidad?</h3>
<div>

    <partial name="../Shared/PartialViews/_UniversityDeleteDetails" />
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />

        <input type="submit" value="Eliminar" class="btn btn-outline-danger" />
        <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </form>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}