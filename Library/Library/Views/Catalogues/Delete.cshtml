@model Library.DAL.Entities.Catalogue

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estás seguro que deseas eliminar este catálogo?</h3>
<div>

    <partial name="../Shared/PartialViews/_CatalogueDeleteDetails" />
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />

        <input type="submit" value="Eliminar" class="btn btn-outline-danger" />
        <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </form>
</div>
